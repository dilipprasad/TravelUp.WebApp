{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft.Extensions.Hosting": "Warning",
        "Default": "Information"
      }
    }
  },
  "RetrySettings": {
    "MaxRetries": 3,
    "DelaySeconds": 2
  },
  "ConnectionStrings": {
    "ServiceEndpoint": "https://localhost:7100/api/"
  }

}